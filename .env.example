# 應用設定 (必需)
NODE_ENV=development
PORT=3000
APP_NAME=one-key-balance-kit

# MongoDB 設定
MONGO_HOST=localhost
MONGO_PORT=27017
MONGO_USERNAME=onekey
MONGO_PASSWORD=onekey123
MONGO_DATABASE=onekey

# Redis 設定
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Web3 設定 (可選)
RPC_URL=https://eth-mainnet.g.alchemy.com/v2/your-api-key
CHAIN_ID=1
API_KEY=your-api-key

# 網路設定 (必需)
NETWORK_TIMEOUT=30000
NETWORK_RETRIES=3

# Alchemy API 密鑰 (針對不同區塊鏈)
# 以太坊 API 密鑰 (可用於主網和測試網，如果未設置特定網絡密鑰)
ALCHEMY_API_KEY_ETH=your-eth-api-key

# 以太坊特定網絡 API 密鑰 (可選，如果設置將優先於通用密鑰)
ALCHEMY_API_KEY_ETH_MAINNET=your-eth-mainnet-api-key
ALCHEMY_API_KEY_ETH_TESTNET=your-eth-testnet-api-key

# Solana API 密鑰 (可用於主網和測試網，如果未設置特定網絡密鑰)
ALCHEMY_API_KEY_SOL=your-sol-api-key

# Solana 特定網絡 API 密鑰 (可選，如果設置將優先於通用密鑰)
ALCHEMY_API_KEY_SOL_MAINNET=your-sol-mainnet-api-key
ALCHEMY_API_KEY_SOL_TESTNET=your-sol-testnet-api-key

# Polygon API 密鑰 (如果有實現)
ALCHEMY_API_KEY_POLYGON=your-polygon-api-key
ALCHEMY_API_KEY_POLYGON_MAINNET=your-polygon-mainnet-api-key
ALCHEMY_API_KEY_POLYGON_TESTNET=your-polygon-testnet-api-key